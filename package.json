{
  "name": "@jcamp/eslint-config",
  "type": "module",
  "version": "1.0.0",
  "description": "JohnCampionJr's ESLint config",
  "author": "John Campion (https://github.com/JohnCampionJr/)",
  "license": "MIT",
  "homepage": "https://github.com/jcamp-code/eslint-config",
  "keywords": [
    "eslint-config",
    "prettier"
  ],
  "exports": {
    ".": "./index.js",
    "./prettier": "./.prettierrc.js"
  },
  "main": "index.js",
  "files": [
    ".prettierrc.js",
    "index.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "format": "prettier . --check",
    "clean": "prettier . --write"
  },
  "peerDependencies": {
    "eslint": ">=8.51.0",
    "prettier": ">=3.0.3"
  },
  "dependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.22",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier-plugin-tailwindcss": "^0.5.5"
  },
  "devDependencies": {
    "eslint": "^8.51.0",
    "prettier": "^3.0.3"
  }
}
