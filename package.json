{
  "name": "@jcamp/eslint-config",
  "type": "module",
  "version": "2.1.0",
  "description": "JohnCampionJr's ESLint config",
  "author": "John Campion (https://github.com/JohnCampionJr/)",
  "license": "MIT",
  "homepage": "https://github.com/jcamp-code/eslint-config",
  "keywords": [
    "eslint-config",
    "prettier"
  ],
  "exports": {
    ".": "./index.js",
    "./prettier": "./.prettierrc.js"
  },
  "main": "index.js",
  "files": [
    ".prettierrc.js",
    "index.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "format": "prettier . --check",
    "clean": "prettier . --write"
  },
  "peerDependencies": {
    "@antfu/eslint-config": ">=2.20.0",
    "eslint": ">=8.0.0",
    "prettier": ">=3.0.3"
  },
  "dependencies": {
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.17.3",
    "prettier-plugin-classnames": "^0.6.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.20.2",
    "eslint": "^9.4.0",
    "prettier": "^3.3.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [],
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
}
